    "description": "Simple visitor tracker and statistics package for Laravel 5 that can be used for dashboard graphs.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksa Nikolic",
            "email": "aleksa@codelab.rs"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "ext-zlib": "*",
        "illuminate/http": "~8.0.0",
        "illuminate/database": "~8.0.0",
        "illuminate/routing": "~8.0.0",
        "illuminate/support": "~8.0.0",
        "piwik/device-detector": "^3.11",
        "maxmind-db/reader": "^1.4"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "Aleksa\\LaravelVisitorsStatistics\\": "src/"
        }
